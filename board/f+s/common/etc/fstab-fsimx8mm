# <file system>	<mount pt>	<type>	<options>	<dump>	<pass>
/dev/root	/			squashfs	ro,noauto	0	1
proc		/proc		proc		defaults	0	0
devpts		/dev/pts	devpts		defaults,gid=5,mode=620,ptmxmode=0666	0	0
tmpfs		/dev/shm	tmpfs		mode=0777	0	0
tmpfs		/tmp		tmpfs		mode=1777	0	0
tmpfs		/run		tmpfs		mode=0755,nosuid,nodev	0	0
sysfs		/sys		sysfs		defaults	0	0
# data partition
/dev/mmcblk2p7 	/rw_fs/root	ext4		defaults	0	0
/dev/ubi0_2 	/rw_fs/root	ubifs		defaults	0	0
overlay		/etc		overlay   defaults,lowerdir=/etc,upperdir=/rw_fs/root/upper/etc,workdir=/rw_fs/root/work/etc  		0		0
overlay		/root		overlay   defaults,lowerdir=/root,upperdir=/rw_fs/root/upper/root,workdir=/rw_fs/root/work/root  	0		0
overlay		/usr		overlay   defaults,lowerdir=/usr,upperdir=/rw_fs/root/upper/usr,workdir=/rw_fs/root/work/usr  		0		0
overlay		/mnt		overlay   defaults,lowerdir=/mnt,upperdir=/rw_fs/root/upper/mnt,workdir=/rw_fs/root/work/mnt  		0		0
